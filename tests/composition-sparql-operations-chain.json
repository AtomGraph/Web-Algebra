{
  "@op": "DESCRIBE",
  "args": {
    "endpoint": {
      "@op": "Uri",
      "args": {
        "input": "https://dbpedia.org/sparql"
      }
    },
    "query": {
      "@op": "Substitute",
      "args": {
        "query": {
          "@op": "Str",
          "args": {
            "input": "DESCRIBE ?person WHERE { ?person <http://xmlns.com/foaf/0.1/name> ?name . ?person <http://dbpedia.org/ontology/birthPlace> <http://dbpedia.org/resource/London> . FILTER(?name = ?targetName) }"
          }
        },
        "var": "targetName",
        "binding": {
          "value": "Caleb Quaye",
          "type": "literal"
        }
      }
    }
  }
}