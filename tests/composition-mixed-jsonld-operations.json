{
  "@op": "Merge",
  "args": {
    "graphs": [
      {
        "@op": "GET",
        "args": {
          "url": {
            "@op": "Uri",
            "args": {
              "input": "http://dbpedia.org/resource/Berlin"
            }
          }
        }
      },
      {
        "@context": {
          "ex": "http://example.org/",
          "foaf": "http://xmlns.com/foaf/0.1/"
        },
        "@id": "http://example.org/person1",
        "@type": "foaf:Person",
        "foaf:name": "Test Person",
        "ex:birthPlace": {
          "@op": "Uri", 
          "args": {
            "input": {
              "@op": "Replace",
              "args": {
                "input": "http://dbpedia.org/resource/CITY_NAME",
                "pattern": "CITY_NAME",
                "replacement": "Berlin"
              }
            }
          }
        }
      },
      {
        "@context": {
          "rdfs": "http://www.w3.org/2000/01/rdf-schema#"
        },
        "@id": "_:metadata",
        "rdfs:label": {
          "@op": "Str",
          "args": {
            "input": "Generated metadata"
          }
        }
      }
    ]
  }
}